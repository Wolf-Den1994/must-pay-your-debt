{
  "expo": {
    "name": "Must pay your debt",
    "slug": "Must pay your debt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3d3d3d",
      "dark": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#3d3d3d"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f6f6f6"
      },
      "package": "com.wolfden.mustpayyourdebt"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f56945a5-0afc-4abe-ae5f-ba6a7f6d2551"
      }
    },
    "owner": "wolf-den"
  }
}
